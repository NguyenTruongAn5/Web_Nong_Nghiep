@model IEnumerable<CategoryModel>

@foreach (var item in Model)
{
    <div class="brands-name">
        <ul class="nav nav-pills nav-stacked">
            <li>
                <a asp-action="Index" asp-controller="Category" asp-route-Slug="@item.Slug">
                    @item.Name
                </a>
            </li>
        </ul>
    </div>
}
